package com.nwl.lanya.dto;

import com.nwl.lanya.po.VideoAudio;
import com.nwl.lanya.vo.VideoAudioVo;

public class VideoAudioDto extends AbstractBaseDto<VideoAudio>{

	/** 
	* @Fields serialVersionUID : TODO(用一句话描述这个变量表示什么) 
	*/ 
	private static final long serialVersionUID = -8930537976457306458L;
	
	//条件查询用
	private VideoAudioVo vo;
	private Integer teacherId;
	//上传视频音频用
	private String httpUrl;
	private Integer type;

	public Integer getTeacherId() {
		return teacherId;
	}

	public void setTeacherId(Integer teacherId) {
		this.teacherId = teacherId;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getHttpUrl() {
		return httpUrl;
	}

	public void setHttpUrl(String httpUrl) {
		this.httpUrl = httpUrl;
	}

	public VideoAudioVo getVo() {
		return vo;
	}

	public void setVo(VideoAudioVo vo) {
		this.vo = vo;
	}
	
}
