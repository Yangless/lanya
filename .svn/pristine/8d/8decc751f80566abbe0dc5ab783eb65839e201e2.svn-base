*{
	margin: 0px;
	padding:0px;
}
body{
	min-height: 1000px;
}
#courseInfo .row div{
	padding: 10px;
}
.showUl{
	font-size: 14px;
	color: #999999;
}
.showUl li{
	display: inline-block;
	margin: 10px;
}
#courseInfo p{
	border-top:1px solid #ededed;
}
.titleSpan{
	display: block;
	font-size: 18px;
	color: #666;
	margin: 10px 0px;
}
.contentSpan{
	font-size: 14px;
	color: #999;
	text-indent: 20px !important;
}
#menuUl{
	margin-top: 50px;
}

#detailDiv{
	margin:5%;
}

#listDiv{
	display: none;
	margin:5%;
}
.courseListClass{
	width: 95%;
	margin:30px auto;
	min-height: 100px;
}
.courseListClass h3{
	font-size: 20px;
}
.courseListClass h3 span{
	margin-right: 10px;
}
.courseListClass div{
	background: #F4F4F4;
	padding:15px 0px;
	margin: 10px 0px;
}
.courseListClass div:hover{
	background: #EEEEEE;
	cursor: pointer;
}
.courseListClass div span{
	vertical-align: middle;
	font-size: 16px;
	color: #666666;
}
.glyphicon,.courseName{
	margin-left:10px;
}

#commentDiv{
	display: none;
	margin:5%;
}
.commentListClass{
	border-top:1px dashed #ededed;
	padding: 20px 0px;
}
.commentListClass div{
	min-height: 60px;
}
.commentListClass dl{
	font-size: 1.2rem;
	color:#777;
}
.commentListClass dl dd span{
	color: cornflowerblue;
}
.commentListClass dl dt{
	margin-top: 10px;
}
.commentP{
	font-size: 1.2rem;
	color: #777777;
}
.timeP{
	font-size: 1.2rem;
	color: #777777;
}
.commentShow img{
	border: 1px solid #ededed;
}
.commentListClass div:last-of-type{
	font-size: 1.2rem;
	color: #777777;
	line-height: 100%;
	text-align: center;
}
.commentListClass div:last-of-type img{
	vertical-align: bottom;
}
#loadMore{
	width: 100%;
	height: 50px;
	padding: 10px;
	text-align: center;
}
#loadMore button:hover{
	color: white;
	background: #26A0DA;
}
#loadMore button{
	border: 1px solid #ededed;
	width: 120px;
	height: 30px;
	background:white;
	color: #777777;
}
/*课程详细*/
.propaedeutics > li{
	margin-left: 5%;
	color: #666666;
}
#studyType > img{
	max-height: 300px;
}
#detailDiv>div{
	padding: 30px 0;
	border-top: 1px #DDDDDD solid;
}
.readOnly>p{
	color: #666;
	font-size: 1.3rem;
}
.prepare{
	color: red;
	font-size: 1.5rem;
}








