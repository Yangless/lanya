body{
	padding-top: 300px;
}
#tearcherInfo{
	text-align: left;
	width: 100%;
	background: #FFFFFF;
	position: fixed;
	top:45px;
	left:0px;
	z-index: 100;
}
.teacherInfoA{
	padding: 10px 0px;
	background:#26A0DA;
	border-bottom-left-radius: 15px;
	border-bottom-right-radius: 15px;
	position: relative;
}
#attention{
	position: absolute;
	top:5px;
	left:5px;
}
#attention span{
	font-size: 14px;
	color: #FFFFFF;
}
#myTeacher{
	position: absolute;
	top:5px;
	right:5px;
}
#myTeacher span{
	font-size: 14px;
	color: #FFFFFF;
}
.teacherPhoto{
	font-size: 14px;
	text-align: center;
	width: 100%;
	height: 90px;
}
.teacherPhoto img{
	width: 50px;
	height: 50px;
	border-radius: 50px;
}
.teacherPhoto div{
	color: white;
}
.teacherPhotoItem{
	width: 100%;
}
.teacherPhotoItem tr td{
	padding-left: 5px;
}
.teacherPhotoItem tr td:first-of-type{
	width: 50px;
}
.teacherPhotoItem img{
	width: 50px !important;
	height: 50px;
	border-radius: 50px;
}
.teacherPhotoItem dt{
	float: left;
}
.teacherPhotoItem dd{
	float: left;
}
.teacherInfoA table{
	width: 90%;
	margin: auto;
	margin-top:40px;
	text-align: center;
}
.teacherInfoA table tr td{
	width: 25%;
	height: 20px;
	color: white;
}
.teacherInfoA table tr td dl dd{
	font-size: 12px;
}
.teacherInfoB{
	padding: 10px;
	color:#26A0DA;
	background: #FFFFFF;
}
.teacherInfoB span{
	margin: 3px;
	font-size: 14px;
}
#tearchContent{
	margin: 10px auto;
	background: #FFFFFF;
}
.mui-control-item{
	font-size: 14px;
	font-weight: 500;
}
#teacherList{
	width: 100%;
	height: 300px;
	background: red;
}
.itemClass{
	width: 100%;
}
.titleClass{
	padding: 20px 0px 5px 0px;
	font-size: 16px;
	color:#26A0DA;
	text-indent: 20px;
	background: #FFFFFF;
	border-bottom: 2px solid #26A0DA;
}
.contentClass{
	
}
.itemShowClass{
	font-size: 14px;
	color:#FFFFFF;
	text-align:center;
	width:100%;
	height:170px;
	background:#26A0DA;
	position: relative;
	padding-top: 20px;
}
.itemShowClass a{
	color: white;
}
.closeClass{
	position: absolute;
	top: 0px;
	left:0px;
}
