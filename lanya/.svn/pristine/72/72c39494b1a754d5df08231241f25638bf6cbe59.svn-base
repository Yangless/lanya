body{
	padding-bottom: 50px;
}
#navDivInput{
	display: none;
}
.mui-search{
	margin-top: 5px;
}
#introDiv{
	padding-top: 50px;
	background: #EFEFF4;
}
.itemClass{
	width: 100%;
}
.titleClass{
	padding: 20px 0px 5px 0px;
	font-size: 16px;
	color:#26A0DA;
	text-indent: 20px;
	background: #FFFFFF;
	border-bottom: 2px solid #26A0DA;
}
.contentClass{
	
}
.itemShowClass{
	font-size: 14px;
	color:#FFFFFF;
	text-align:center;
	width:100%;
	height:170px;
	background:#26A0DA;
	position: relative;
	padding-top: 20px;
}
.itemShowClass a{
	color: white;
}
.closeClass{
	position: absolute;
	top: 0px;
	left:0px;
}
.itemShowClassLast{
	font-size: 50px;
	color:#26A0DA;
	width:100%;
	height:170px;
	background:#FFFFFF;
	border:1px dashed #26A0DA;
	line-height:160px;
	text-align:center;
}
.itemShowInfoClass{
	width: 100%;
	position: absolute;
	left:0px;
	bottom:10px;
}
.contactClass{
	width: 90%;
	margin: auto;
	height:120px;
	border-radius:5px;
	margin-top: 10px;
	box-shadow: 5px 5px 5px 3px grey;
}
.showAd{
	font-size: 12px !important;
}
.infoSpan{
	display: inline-block;
}
#myReport{
	display: none;
}
#userInfo{
	width: 100%;
	height: 100px;
	background: #26A0DA;
	color: white;
	border-bottom-left-radius: 20px;
	border-bottom-right-radius: 20px;
	text-align: center;
}
#userInfo img{
	margin-top: 10px;
	width: 50px;
	height: 50px;
	border-radius:100%;
}
#userInfo p{
	color: white;
}
.mui-table-view{
	margin-bottom: 10px;
	font-size: 16px;
	color: #26A0DA;
}
.contentTable{
	width: 100%;
	margin-bottom: 10px;
}
.contentTable tr{
	border-bottom: 1px dashed #ededed;
}
.contentTable tr td{
	width: 100%;
	height: 45px;
	padding: 8px;
	font-size:0.825rem !important;
	text-align: center;
}
#logoutBtn{
	width: 90%;
	margin:auto;
}









