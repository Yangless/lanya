.progressBorder{
    width:100%;
    height:20px;
    border:1px solid #26A0DA;
    text-align:right;
    position: relative;
    top:20px;
}
.progressDiv{
    width:0px;
    height:20px;
    background:#26A0DA;
    position:absolute;
    top:0px;
    left:0px;
    z-index:50;
}
.progressDiv span{
   line-height:20px;
   color:black;
}