*{
	margin: 0px;
	padding: 0px;
	font-size: 14px;
}
body{
	background: #EFEFF4;
}
li{
	list-style: none;
}
table{
	border-collapse:collapse;
}
.logDiv{
	width: 100%;
	padding-top: 3px;
	padding-bottom: 3px;
	text-align:center;
	background: #FFFFFF;
}
.logDiv img{
	width: 30px;
	height: 30px;
}
.logDiv span:first-of-type{
	color:#26A0DA;
	font-size:16px;
}
#navDiv{
	position: fixed;
	top:0px;
	left:0px;
	z-index: 100;
}
#navDivInput{
	padding: 0px 10px;
	background: #FFFFFF;
}
.mui-search{
	margin-top: 5px;
}
#soundSearch{
	margin-top: -10px;
	width: 50px;
	height: 50px;
	border-radius: 50px;
}
.mui-card-footer span{
	font-size: 18px !important;
}
.numberSpan{
	margin-left: 5px;
}
.mui-table-view{
	margin-bottom: 10px;
	font-size: 16px;
	color: #26A0DA;
}