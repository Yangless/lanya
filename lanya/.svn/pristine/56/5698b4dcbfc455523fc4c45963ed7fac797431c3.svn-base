*{
	margin: 0px;
	padding: 0px;
}
li{
	list-style: none;
}
#videoDiv{
	width: 80%;
	margin: auto;
}
#videoDiv video{
	width: 100%;
}
#menuUl{
	position: fixed;
	top:20%;
	left:0px;
	width: 80px;
	background: rgba(0,0,0,0.5);
}
#menuUl li{
	text-align: center;
	line-height: 50px;
	height: 50px;
	border: 1px solid white;
	font-size: 16px;
	color: white;
}
#menuUl li:hover{
	cursor: pointer;
}
#commentDialog table{
	width:80%;
	margin: auto;
}
#commentDialog table tr td{
	padding: 5px;
	width:50%; 
	text-align: left;
}










